  "query": "\n            SELECT \n                p.ticker, \n                a.name, \n                p.quantity, \n                p.avg_buy_price, \n                a.current_price\n            FROM portfolio p\n            LEFT JOIN assets a ON p.ticker = a.ticker\n            WHERE p.user_id = $1\n            ",
